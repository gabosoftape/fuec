<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE xml>
    <odoo>
        <record model="ir.module.category" id="module_category_alistamientos">
                <field name="name">Control de alistamientos</field>
                <field name="description">Ayuda a administrar los usuarios del modulo </field>
            </record>

        <!--Group for Enlist admin  -->
        <record id="group_enlist_admin" model="res.groups">
            <field name="name">Administrador</field>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="category_id" ref="module_category_alistamientos"/>
        </record>

        <!--Group for Enlist user -->
        <record id="group_enlist_user" model="res.groups">
            <field name="name">Usuario</field>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="category_id" ref="module_category_alistamientos"/>
        </record>
    </odoo>